{
  "name": "coc-go",
  "version": "0.1.0",
  "description": "gopls extension for coc",
  "author": "josa@gesell.me",
  "license": "MIT",
  "main": "lib/extension.js",
  "publisher": "josa42",
  "engines": {
    "coc": "^0.0.65"
  },
  "keywords": [
    "coc.nvim",
    "languageserver",
    "gopls"
  ],
  "scripts": {
    "clean": "rimraf lib",
    "build": "tsc -p tsconfig.json",
    "build:watch": "tsc --watch -p tsconfig.json",
    "prepare": "yarn clean && yarn build"
  },
  "activationEvents": [
    "onLanguage:go"
  ],
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "gols",
      "properties": {
        "go.enable": {
          "type": "boolean",
          "default": true
        },
        "go.commandPath": {
          "type": "string"
        }
      }
    },
    "commands": [
      {
        "command": "go.installGopls",
        "title": "Install / Update gopls"
      },
      {
        "command": "go.format.imports",
        "title": "Format imports"
      }
    ]
  },
  "devDependencies": {
    "@chemzqm/tsconfig": "^0.0.3",
    "@chemzqm/tslint-config": "^1.0.17",
    "@types/node": "^10.14.5",
    "coc.nvim": "0.0.62",
    "rimraf": "^2.6.2",
    "tslint": "^5.11.0",
    "typescript": "^3.0.3"
  },
  "dependencies": {
    "@types/which": "^1.3.1",
    "tslib": "^1.9.3",
    "which": "^1.3.1"
  }
}
